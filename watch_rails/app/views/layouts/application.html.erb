<!DOCTYPE html>
<html lang="en">
  <head>

    <%= display_meta_tags title: @page_title , descriptions: @page_descriptions, keywords: @page_keywords %>    

    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>



    <%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload' %>
    <%= stylesheet_pack_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
</head>

  <body>
    <div class="content">
      <%= yield %>
    </div>
  
  
  
  
  
  
  
  
  
    <script src="<%= asset_path 'application' %>">
    </script>
  </body>
</html>
